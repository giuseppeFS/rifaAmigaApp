<div  class="login-wrap">
  <%= form_with(url: sign_in_path, html: { class: "form-horizontal" }, method: "post") do %>
  <div class="form-group">
    <h2>Login</h2>
    <%= label :cgc, :title, "CPF/CNPJ" %><br />
    <input type="text" name="cgc" id="cgc">
  </div>

  <div class="form-group">
    <%= label :password, :title, 'Senha' %><br />
    <input type="password" name="password" id="password">
  </div>

  <div class="form-group">
    <div class="actions">
      <%= submit_tag "Entrar" %>
    </div>
  </div>

  <% end %>

</div>