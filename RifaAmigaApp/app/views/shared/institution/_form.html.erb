</div>


<%= f.label :ddd_phone2 %>
<div class="input-group">
	<%= f.number_field :ddd_phone2, :class => 'form_control' %>
</div>


<%= f.label :phone_number2 %>
<div class="input-group">
	<%= f.number_field :phone_number2, :class => 'form_control' %>
